{
  "name": "Discord Theme Loader",
  "version": "1.0",
  "description": "A Chrome extension that allows users to upload .CSS themes in Discord.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["contentscript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
